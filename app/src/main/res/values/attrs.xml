<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextView">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="integer" />
    </declare-styleable>


    <declare-styleable name="QQStepView">
        <attr name="stepMax" format="integer" />
        <attr name="currentStep" format="integer" />
        <attr name="bounds" format="dimension" />
        <attr name="outerColor" format="color" />
        <attr name="innerColor" format="color" />
        <attr name="stepColor" format="color" />
        <attr name="stepSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ColorTrackView">
        <attr name="originColor" format="color" />
        <attr name="changeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="progressOuterColor" format="color" />
        <attr name="progressInnerColor" format="color" />
        <attr name="progressBoundWidth" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="starNum" format="integer" />
        <attr name="starInterval" format="dimension"/>
        <attr name="star" format="reference" />
        <attr name="starDownVote" format="reference" />
        <attr name="starLike" format="reference" />
        <attr name="starUpVote" format="reference" />
    </declare-styleable>

    <declare-styleable name="LoadView">
        <attr name="loadSize" format="dimension" />
    </declare-styleable>


</resources>